{
  "name": "ye-baileys",
  "version": "7.5.6-rc.5",
  "description": "yehazkiel modified Baileys WhatsApp API for custom bots and personal projects",
  "keywords": [
    "baileys",
    "whatsapp",
    "whatsapp-api",
    "whatsapp-web",
    "whatsapp-bot",
    "automation",
    "multi-device",
    "libsignal",
    "yehazkiel"
  ],
  "homepage": "https://github.com/yehazkiell/ye-baileys",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yehazkiell/ye-baileys.git"
  },
  "license": "MIT",
  "author": "yehazkiel",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "WAProto/*",
    "engine-requirements.js"
  ],
  "scripts": {
    "build:all": "tsc && typedoc",
    "build:docs": "typedoc",
    "build:tsc": "tsc",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "example": "node --inspect -r ts-node/register Example/example.ts",
    "gen:protobuf": "sh WAProto/GenerateStatics.sh",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint src --fix --ext .js,.ts,.jsx,.tsx",
    "prepack": "echo 'yehazkiell Baileys'",
    "prepare": "echo 'yehazkiell Baileys'",
    "preinstall": "node ./engine-requirements.js",
    "release": "release-it",
    "test": "jest"
  },
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@cacheable/node-cache": "^1.4.0",
    "@hapi/boom": "^10.0.1",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "async-mutex": "^0.5.0",
    "audio-decode": "^2.1.3",
    "axios": "^1.6.0",
    "cache-manager": "^7.2.4",
    "chalk": "^5.6.2",
    "cheerio": "^1.0.0-rc.10",
    "gradient-string": "^3.0.0",
    "libphonenumber-js": "^1.10.20",
    "libsignal": "npm:@scrtlabs/libsignal-node",
    "lodash": "^4.17.21",
    "music-metadata": "^11.9.0",
    "pino": "^10.1.0",
    "protobufjs": "^7.5.4",
    "uuid": "^13.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@adiwajshing/eslint-config": "github:adiwajshing/eslint-config",
    "@types/got": "^9.6.11",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.0",
    "@types/ws": "^8.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "jimp": "^1.6.0",
    "json": "^11.0.0",
    "link-preview-js": "^3.0.0",
    "open": "^10.2.0",
    "qrcode-terminal": "^0.12.0",
    "release-it": "^19.0.5",
    "sharp": "^0.34.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.8.1",
    "typedoc": "^0.28.14",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "jimp": "^1.6.0",
    "link-preview-js": "^3.0.0",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.34.4"
  },
  "peerDependenciesMeta": {
    "jimp": {
      "optional": true
    },
    "link-preview-js": {
      "optional": true
    },
    "qrcode-terminal": {
      "optional": true
    },
    "sharp": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/yehazkiell/ye-baileys/issues"
  }
}
